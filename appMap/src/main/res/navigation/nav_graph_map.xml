<?xml version="1.0" encoding="utf-8"?>
<navigation  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation"
    app:startDestination="@id/authFragment">

    <fragment
        android:id="@+id/authFragment"
        android:name="com.example.auth.presentation.AuthFragment"
        android:label="AuthFragment"
        tools:layout="@layout/fragment_auth"/>

   <!-- <action
        android:id="@+id/action_authFragment_to_map"
        app:destination="@id/map" />-->
    <action
        android:id="@+id/action_track_to_map"
        app:destination="@id/map"/>

    <action
        android:id="@+id/action_map_to_authFragment"
        app:destination="@id/authFragment" />
    <fragment
        android:id="@+id/map"
        android:name="com.example.map.presentation.MapFragment"
        android:label="MapFragment"
        tools:layout="@layout/fragment_map" />

    <action
        android:id="@+id/action_map_to_track"
        app:destination="@id/track" />

    <fragment
        android:id="@+id/track"
        android:name="com.example.tracker.presentation.TrackerFragment"
        android:label="TrackFragment"
        tools:layout="@layout/fragment_tracker" />
</navigation>